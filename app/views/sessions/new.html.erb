<div class="jumbotron">
  <div class="container">
  <h3>Log in</h3>
  <%= form_with(scope: :session, url: sessions_path) do |f| %>

    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Log in" %>
  <% end %>
  </div>
</div>
